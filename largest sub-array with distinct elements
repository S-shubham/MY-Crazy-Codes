#include<iostream>
using namespace std;

int main()
{
	int arr[]={2,4,1,1,6,8,3,6,4,7,15,3};
	
	int start=0,end=0,max=1,last=0;
	
	for(int i=0;i<12;i++)
	{
		for(int j=last;j<i;j++)
		{
			if(arr[i]==arr[j])
			{
				if(max<(i-last))
				{
					max=i-last;
					start=last;
					end=i-1;
				}
				last=i;
				break;
			}
			
		}
		if((i-last+1)>(end-start))
		{
			start=last;
			end=i;
		}
	}
	cout<<"\nMax length is : "<<(end-start+1);
	cout<<"\nThe subarray is : ";
	for(int i=start;i<=end;i++)
	cout<<arr[i]<<" ";
}
