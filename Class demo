// Goal is to create a class with 10 objects
#include<bits/stdc++.h>
using namespace std;

class A{ 
	public:
		int a=10;
		string str;
		A()
		{
			str="Hello I'm default";
		}
		void display()
		{
			cout<<"\nThe value of a is : "<<a<<" and the value of string is : "<<str;
		}
};

int main()
{
	A obj[10];
	for(int i=0;i<10;i++)
	obj[i].display();
	
}
